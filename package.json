{
  "name": "redux-batched-subscribe",
  "version": "0.1.6",
  "description": "redux store enhancer which allows batching subscribe notifications.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib && cp src/index.d.ts lib",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel/register --recursive src/**/*-test.js",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "redux",
    "batched",
    "react",
    "reactjs",
    "performance",
    "store"
  ],
  "author": "Terry Appleby <terry.appleby@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tappleby/redux-batched-subscribe.git"
  },
  "bugs": {
    "url": "https://github.com/tappleby/redux-batched-subscribe/issues"
  },
  "homepage": "https://github.com/tappleby/redux-batched-subscribe",
  "devDependencies": {
    "babel": "^5.8.20",
    "babel-core": "^5.8.20",
    "eslint": "^1.0.0",
    "eslint-config-airbnb": "2.1.1",
    "expect": "^1.13.4",
    "mocha": "^2.2.5",
    "rimraf": "^2.5.0"
  },
  "peerDependencies": {
    "redux": ">=1.0.0"
  }
